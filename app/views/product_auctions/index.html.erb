<h1>Listing product_auctions</h1>

<table>
  <thead>
    <tr>
      <th>Start time</th>
      <th>End time</th>
      <th>M food</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @product_auctions.each do |product_auction| %>
      <tr>
        <td><%= product_auction.start_time %></td>
        <td><%= product_auction.end_time %></td>
        <td><%= product_auction.m_food_id %></td>
        <td><%= link_to 'Show', product_auction %></td>
        <td><%= link_to 'Edit', edit_product_auction_path(product_auction) %></td>
        <td><%= link_to 'Destroy', product_auction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

